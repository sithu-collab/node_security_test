<%- include("partials/header") %>
    <div class="container">
        <div class="jumbotron text-center">
            <i class="fas fa-key fa-6x"></i>
            <h1 class="display-3">You've discovered my secrets!</h1>
            <% usersWithSecrets.forEach(user => { %>
                <p class="lead secret-text"><%= user.secret %></p>
    
            <% }); %>
            <hr>
            <a class="btn btn-light btn-lg" role="button" href="/logout">Log Out</a>
            <a class="btn btn-dark btn-lg" role="button" href="/submit">Submit a Secret</a>
        </div>
    </div>
<%- include("partials/footer")  %> 